     // TODO Tree
     "todo-tree.tree.flat": true,
     "todo-tree.tree.buttons.filter": false,
     "todo-tree.tree.expanded": true,
     "todo-tree.tree.showCountsInTree": true,
     "todo-tree.tree.showCurrentScanMode": false,
     "todo-tree.tree.buttons.viewStyle": false,
     "todo-tree.tree.buttons.groupByTag": false,
     "todo-tree.tree.buttons.expand": false,
     "todo-tree.tree.disableCompactFolders": true,
     "todo-tree.tree.labelFormat":
         "${line}:${column} ${after}",
     "todo-tree.highlights.defaultHighlight":{
         "foreground": "white",
         "background": "grey",
         "type": "tag-and-comment",
         "icon": "$(circle-large)"
     },
     "todo-tree.highlights.customHighlight": {
         "COMPLETED": {
             "foreground": "#FFFFFF",
             "background": "#AAD94D",
             "type": "tag-and-comment",
         	"icon": "$(pass)"
         },
         "TODO": {
             "foreground": "#FFFFFF",
             "background": "#feeb59",
             "type": "tag-and-comment",
         },
         "BUG": {
             "foreground": "#FFFFFF",
             "background": "#F07178",
             "type": "whole-line",
         	"icon": "$(debug)"
         },
         "NOTE": {
             "foreground": "#FFFFFF",
             "background": "#3DBAE6",
             "type": "tag-and-comment",
             "icon": "$(info)"
         }
     },
     "todo-tree.general.tags": ["BUG","NOTE","TODO","COMPLETED","WARNING","FIXME","FIX","[ ]","[x]","[√]","COMEBACK","ERROR","PENDING","INFO",],
     "todo-tree.general.tagGroups": {
         "BUG"       :["BUG", "WARNING", "ERROR", "FIXME", "FIX"],
         "TODO"      :["TODO", "COMEBACK", "[ ]", "PENDING"],
         "COMPLETED" :["COMPLETED", "[x]", "[√]"],
         "NOTE"       :["NOTE", "INFO", ""]
     },

